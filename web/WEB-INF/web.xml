<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>photoServlet</servlet-name>
        <servlet-class>images.photoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>photoServlet</servlet-name>
        <url-pattern>/images/photoServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>PhotoServlet2</servlet-name>
        <servlet-class>images.PhotoServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PhotoServlet2</servlet-name>
        <url-pattern>/images/PhotoServlet2</url-pattern>
    </servlet-mapping>

</web-app>